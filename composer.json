{
    "name": "yassine-as/laravel-cas-auth",
    "description": "CAS Authentication package for Laravel applications",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Yassine ait sidi brahim",
            "email": "yassineaitsidibrahim@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.2",
        "laravel/framework": "^8.0|^9.0|^10.0",
        "subfission/cas": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "YassineAs\\CasAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YassineAs\\CasAuth\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YassineAs\\CasAuth\\Providers\\CasAuthServiceProvider"
            ],
            "aliases": {
                "CasAuth": "YassineAs\\CasAuth\\Facades\\CasAuth"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}